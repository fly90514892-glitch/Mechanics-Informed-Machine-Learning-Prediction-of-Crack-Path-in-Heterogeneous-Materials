#!/bin/bash -l
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=1
#SBATCH --mem-per-cpu=8G
#SBATCH --job-name=predict
#SBATCH --partition=standard
#SBATCH --time=7-00:00:00
#SBATCH --output=predict.out

sbatch --dependency=$(squeue --noheader --format %i --name predict) combine_predict.qsub

vpkg_require singularity/3.11.1
export SINGULARITY_IMAGE=/work/ud_zlab/sw/tensorflow/tensorflow_2.12.0-gpu.sif
export SINGULARITY_OPTS="--nv $SINGULARITY_OPTS"

Sexec python ./predict.py
